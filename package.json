{
    "name": "management",
    "version": "1.0.0",
    "scripts": {
        "start": "nodemon app.js", 
        "client": "cd client && yarn start",
        "server": "nodemon server.js",  
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "body-parser": "2.2.0",
        "express": "^4.21.2"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "nodemon": "^3.1.9"
    }
}
